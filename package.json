{
  "name": "imperial-entanglements",
  "version": "0.0.1",
  "description": "VRE for Imperial Entanglements project",
  "main": "index.js",
  "scripts": {
    "dist": "build",
    "tsc": "./node_modules/.bin/tsc",
    "tsc:watch": "./node_modules/.bin/tsc --watch",
    "electron": "./node_modules/.bin/electron ./build/app",
    "asar": "asar pack build bin/falcon.asar",
    "test": "./node_modules/.bin/mocha",
    "snyk": "snyk test",
    "migrate": "./node_modules/.bin/knex migrate:latest",
    "seed": "./node_modules/.bin/knex seed:run",
    "style": "./node_modules/.bin/node-sass ./client/sass/app.scss --include-path node_modules -o build/static",
    "style:watch": "./node_modules/.bin/node-sass --watch ./client/sass/app.scss --include-path node_modules -o build/static"
  },
  "repository": {
    "type": "git",
    "url": "https://actechlab.warwick.ac.uk/u1572229/imperial-entanglements"
  },
  "build": {
    "appId": "Imperial Entanglements",
    "mac": {
      "category": "public.app-category.education"
    },
    "win": {
      "iconUrl": "http://www.iconarchive.com/download/i43620/treetog/junior/folder-documents.ico"
    }
  },
  "directories": {
    "output": "bin",
    "app": "build/app",
    "buildResources": "tmp"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "jsonschema": "^1.1.0",
    "knex": "^0.12.1",
    "koa": "^1.2.4",
    "koa-bodyparser": "^2.2.0",
    "koa-json": "^1.1.3",
    "koa-qs": "^2.0.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.16.1",
    "normalize.css": "^5.0.0",
    "pg-promise": "^5.3.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-jsonschema-form": "^0.40.0",
    "react-router": "^4.0.0-alpha.4",
    "react-select": "^1.0.0-rc.2",
    "react-toolbox": "^1.2.1",
    "reflect-metadata": "^0.1.8",
    "sqlite3": "^3.1.4"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "electron": "^1.4.0",
    "electron-builder": "^7.10.2",
    "electron-rebuild": "^1.2.1",
    "mocha": "^3.0.2",
    "node-sass": "^3.10.0",
    "snyk": "^1.19.1",
    "tslint": "^3.15.1",
    "tslint-microsoft-contrib": "^2.0.10",
    "typescript": "^2.0.2",
    "webpack": "^1.13.2"
  },
  "snyk": true
}
